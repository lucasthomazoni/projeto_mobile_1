<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.grupodois.conversaoapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ActivityInicial">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TelaDois"
            android:label="@string/conversao" />
        <activity
            android:name=".TelaTres"
            android:label="@string/config" />
        <activity
            android:name=".TelaQuatro"
            android:label="@string/moedas" />
        <activity android:name=".TelaCinco"
            android:label="@string/sobre_o_app"/>
    </application>

</manifest>